{
  "name": "govconnect-ai-service",
  "version": "1.0.0",
  "description": "AI Orchestrator Service for GovConnect - Stateless service that processes WhatsApp messages with LLM",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "npx prisma generate && tsc",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:studio": "npx prisma studio"
  },
  "keywords": [
    "govconnect",
    "ai",
    "orchestrator",
    "gemini",
    "rabbitmq"
  ],
  "author": "GovConnect Team",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.19.0",
    "amqplib": "^0.10.9",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mammoth": "^1.11.0",
    "multer": "^2.0.2",
    "opossum": "^9.0.0",
    "officeparser": "^3.0.3",
    "pdf.js-extract": "^0.2.1",
    "prisma": "^6.19.0",
    "prom-client": "^15.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "textract": "^2.5.0",
    "winston": "^3.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@types/amqplib": "^0.10.8",
    "@types/express": "^5.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/opossum": "^8.1.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.19.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
